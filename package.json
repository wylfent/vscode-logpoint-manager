{
	"name": "logpoint-enable",
	"displayName": "Logpoint Enable",
	"description": "Extension that will help you toggle breakpoint by breakpoint type.",
	"publisher": "wylfent",
	"repository": {
		"type": "git",
		"url": "https://github.com/wylfent/vscode-logpoint-manager.git"
	},
	"icon": "resources/logpoint-icon.png",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "logpoint-enable.enable-nonbreak-logpoints",
				"title": "Debug-Logpoints: Enable Non-break Logpoints"
			},
			{
				"command": "logpoint-enable.enable-all-logpoints",
				"title": "Debug-Logpoints: Enable All Logpoints"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
